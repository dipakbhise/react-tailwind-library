{
  "name": "react-tailwind-library",
  "version": "0.5.7",
  "private": false,
  "description": "React UI Components using TailwindCSS",
  "author": "dipakbhise420@gmail.com",
  "license": "MIT",
  "keywords": [
    "combobox",
    "form",
    "input",
    "multiselect",
    "react",
    "react-component",
    "react-tailwind",
    "select",
    "tailwind",
    "tailwindcss",
    "ui",
    "react",
    "tailwind",
    "components",
    "ui"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipakbhise/react-tailwind-library.git"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:package": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "build:css": "tailwindcss -o dist/style.css --minify",
    "publish": "npm publish",
    "patch": "npm version patch && npm publish",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-lib": "rollup -c"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-babel": "^6.0.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.2",
    "postcss": "^8.4.19",
    "rollup": "^2.38.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.2.4"
  }
}
